<tr>
    <td>
        <div class="table-data__info">
            <h6>
                <%= user.fullname %>
            </h6>
            <span>
                <a href="#">
                    <%= user.email %>
                </a>
            </span>
        </div>
    </td>
    <td>
        <div class="table-data__info">
            <h6>
                <%= user.assign %>
            </h6>
        </div>
    </td>
    <td>
        <div class="table-data__info">
            <h6>
                <%= user.contact %>
            </h6>
        </div>
    </td>
    <td>
        <span class="role <%= user.role %>">
            <%= user.role %>
        </span>
    </td>
    <td>
        <div class="table-data__info">
            <h6>
                <%= user.address %>
            </h6>
        </div>
    </td>
    <td>
        <div class="dropdown">
            <span class="more justify-content-center" id="dropdownMenuButton" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <i class="zmdi zmdi-more "></i>
            </span>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <form action="dashboard/user/delete" method="post">
                    <input type="hidden" name="userId" value="<%= user._id %>">
                    <a class="dropdown-item" href="user/<%= user._id %>">Edit</a>
                    <button class="dropdown-item">Delete</button>
                </form>
            </div>
        </div>
    </td>
</tr>