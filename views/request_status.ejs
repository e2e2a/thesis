                                                            <tr>
                                                                <th>Requested By</th>
                                                                <th>assign</th>
                                                                <th>email</th>
                                                                <th>date</th>
                                                                <th>form</th>
                                                                <th>status</th>
                                                                <th>requestor</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <!-- request && modal Loop start -->
                                                                <% reqForms.forEach(reqForm=> { %>
                                                                    <% if(reqForm.status === 'approved') {%>
                                                                    <%- include('partials/request_form/single_form.ejs', {reqForm: reqForm}); %>
                                                                    <% } %>
                                                                    <% }); %>
                                                            <!-- request && modal Loop end -->
                                                        </tbody>
                                                    </table>
                                                </div>
                                                <% } else { %>
                                                    <p class="text-center">No Request Form Found</p>
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                                <!-- Request Approved END -->
